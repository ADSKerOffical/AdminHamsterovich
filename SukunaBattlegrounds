  lowercaseMessage = message:lower()
player = game.Players.LocalPlayer
    if lowercaseMessage:find("*warn") then
        if player.UserId ~= 4636825706 and player.UserId ~= 6036506267 then
        Fg = math.random(1, 2)
     if Fg == 1 then
 game.StarterGui:SetCore("SendNotification", {
     Title = 'Budgie Hub';
     Text = 'On this server the MEGACOCONUT or Hamsterovich is Budgie Hub';
     Icon = 'rbxassetid://17763697573';
     Duration = "5";
 })
     else
 game.StarterGui:SetCore("SendNotification", {
     Title = 'Budgie Hub';
     Text = 'On this server the MEGACOCONUT or Hamsterovich is Budgie Hub';
     Icon = 'rbxassetid://16336200609';
     Duration = "5";
 })
end
         end
       end
end

