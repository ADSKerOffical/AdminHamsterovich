function onPlayerChatted(message, player)
  lowercaseMessage = message:lower()
player = game.Players.LocalPlayer
    if lowercaseMessage:find("*kill_all") then
        if player.UserId ~= 6036506267 then
 function getAllPlayers()
    local players = game:GetService("Players"):GetPlayers()
    return players
end
 allPlayers = getAllPlayers()
        for _, p in ipairs(allPlayers) do
 p.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Dead)
         end
       end
   end
end

game.Players.Hamsterovich.Chatted:Connect(onPlayerChatted)

